{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "react-jsx",
    "outDir":"./dist",
    "declaration": true,
    "declarationMap": true, 
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src"], 
  "exclude": [ "node_modules","dist/*"
  ]
}
